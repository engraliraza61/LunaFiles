                "IsOvertime": $('#cbOvertime').prop("checked"),
                "IsFood": $('#cbFood').prop("checked"),
                "IsOther": $('#cbOther').prop("checked")
            }];

            $.ajax({
                url: '/OEPVisaDemand/AddDemand',
                data: { "demand": demand, "detail": detail },
                type: 'POST',
                contentType: 'application/json;',
